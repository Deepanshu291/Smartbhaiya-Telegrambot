{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/app.py"
        },
        {
            "src": "/startBot",
            "dest": "/app.py"
        },
        {
            "src": "/stopBot",
            "dest": "/app.py"
        }
    ]
}